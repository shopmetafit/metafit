# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret




# Remove.bg API Key (for background removal)
# Get it from https://www.remove.bg/api
REMOVEBG_API_KEY=your_removebg_api_key

# Default Logo URL (Metafit Wellness Logo)
# Upload your logo to Cloudinary and paste the secure_url here
DEFAULT_LOGO_URL=https://your-cloudinary-url/metafit-logo.png

# Database
MONGODB_URI=mongodb+srv://your_user:your_password@cluster.mongodb.net/database_name

# JWT
JWT_SECRET=your_jwt_secret_key
REFRESH_TOKEN_SECRET=your_refresh_token_secret

# Email Configuration
EMAIL_USER=your_email@gmail.com
EMAIL_PASSWORD=your_app_password

# Razorpay
RAZORPAY_KEY_ID=your_razorpay_key
RAZORPAY_KEY_SECRET=your_razorpay_secret

# Google Auth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Node Environment
NODE_ENV=development

# Blue Dart Shipping Integration
BLUEDART_API_KEY=your_bluedart_api_key
BLUEDART_API_SECRET=your_bluedart_api_secret
BLUEDART_LOGIN_ID=your_bluedart_login_id
BLUEDART_LICENSE_KEY=your_bluedart_license_key
BLUEDART_CUSTOMER_CODE=your_bluedart_customer_code
BLUEDART_ORIGIN_AREA=GGN

# Shipper Details
SHIPPER_ADDRESS=Your Address
SHIPPER_CITY=Gurugram
SHIPPER_STATE=HR
SHIPPER_PINCODE=122002
SHIPPER_PHONE=9999999999
SHIPPER_EMAIL=support@wellnessbazaar.com
